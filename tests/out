mpicxx			 -Wall -O3 -D__COMPUTE_CAPABILITY__=350 -DMULTI_GPU -DGPU_TWISTED_MASS_DIRAC -DGPU_WILSON_DIRAC -DGPU_DIRECT -DBUILD_QDP_INTERFACE -DBUILD_MILC_INTERFACE -DNUMA_AFFINITY -I/usr/local/cuda/include -DMPI_COMMS  -I/usr/lib64/openmpi/include -I../include -I.  dslash_test.cpp -c -o dslash_test.o
